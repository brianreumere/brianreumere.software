[Unit]
Description=Deluge Bittorrent Client Web Interface
Documentation=man:deluge-web
After=deluged.service
Wants=deluged.service

[Service]
Type=simple
UMask=027

User=deluge
Group=deluge

ExecStart=/usr/bin/deluge-web -d -l /var/log/deluge/web.log -L warning -i 127.0.0.1

Restart=on-failure

[Install]
WantedBy=multi-user.target
